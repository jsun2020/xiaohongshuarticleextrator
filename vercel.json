{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 10 }
    },
    {
      "src": "api/auth_status.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 10 }
    },
    {
      "src": "api/test_db.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auth/login.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 15 }
    },
    {
      "src": "api/auth/register.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 15 }
    },
    {
      "src": "api/auth/logout.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 10 }
    },
    {
      "src": "api/xiaohongshu/note.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 30 }
    },
    {
      "src": "api/xiaohongshu_notes_list.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 30 }
    },
    {
      "src": "api/xiaohongshu_recreate.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 60 }
    },
    {
      "src": "api/xiaohongshu_recreate_history.py",
      "use": "@vercel/python",
      "config": { "maxDuration": 30 }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}